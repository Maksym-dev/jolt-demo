<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">

    <!--
        application and version are no longer specified here
        in fact it complains if they are here.
        Version is specified by a human when running the upload/deploy to cloud process.
    -->
    <threadsafe>true</threadsafe>

    <runtime>java8</runtime>  <!-- yes pls ;) -->

    <system-properties>
        <property name="java.util.logging.config.file" value="WEB-INF/logging.properties"/>
    </system-properties>
</appengine-web-app>
